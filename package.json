{
  "name": "aito-grocery-store-demo-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "openai": "^5.6.0",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.3",
    "reactstrap": "^9.2.3",
    "recharts": "^2.15.0"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "start:backend": "node server.js",
    "start:dev": "concurrently \"npm run start:backend\" \"npm start\"",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,md}",
    "generate-data": "node src/generator/index.js",
    "upload-data": "node upload-data.js",
    "upload-data:dry-run": "node upload-data.js --dry-run",
    "screenshots": "node scripts/screenshot-generator.js",
    "screenshots:marketing": "node scripts/screenshot-generator.js landingPage highResMarketing mobileViews",
    "screenshots:tutorials": "node scripts/screenshot-generator.js searchFeatures recommendations invoiceProcessing chatInterface",
    "screenshots:docs": "node scripts/screenshot-generator.js adminDashboard",
    "test:screenshots": "npx playwright test tests/screenshots.spec.js",
    "screenshots:all": "node scripts/screenshot-all-features.js",
    "screenshots:placeholders": "node scripts/create-placeholder-screenshots.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@aitodotai/json-stringify-pretty-compact": "^1.3.0",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "knuth-shuffle-seeded": "^1.0.6",
    "prettier": "^3.4.2",
    "seed-random": "^2.2.0"
  }
}
